#: ../../../LangRef.rst:9375
msgid "In0"
msgstr ""

#: ../../../LangRef.rst:9272 ../../../LangRef.rst:9323
#: ../../../LangRef.rst:9375
msgid "In1"
msgstr ""

#: ../../../LangRef.rst:9272 ../../../LangRef.rst:9323
#: ../../../LangRef.rst:9375
msgid "Out"
msgstr ""

