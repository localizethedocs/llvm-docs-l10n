msgstr ""

#: ../../../LangRef.rst:5633
msgid "``J``: An immediate unsigned 16-bit integer, shifted left 16 bits."
msgstr ""

#: ../../../LangRef.rst:5635
msgid "``L``: An immediate signed 16-bit integer, shifted left 16 bits."
msgstr ""

#: ../../../LangRef.rst:5636
