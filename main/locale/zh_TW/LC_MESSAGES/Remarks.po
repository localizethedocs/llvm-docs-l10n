#: ../../../Remarks.rst:203
msgid "YAML metadata"
msgstr ""

#: ../../../Remarks.rst:205
msgid "The metadata used together with the YAML format is:"
msgstr ""

#: ../../../Remarks.rst:207
msgid "a magic number: \"REMARKS\\\\0\""
msgstr ""

#: ../../../Remarks.rst:208
msgid "the version number: a little-endian uint64_t"
msgstr ""

