#: ../../../Remarks.rst:315 ../../../Remarks.rst:317 ../../../Remarks.rst:339
#: ../../../Remarks.rst:341
msgid "u32"
msgstr ""

#: ../../../Remarks.rst:317 ../../../Remarks.rst:341
msgid "Column"
msgstr ""

#: ../../../Remarks.rst:320
msgid "``RECORD_REMARK_HOTNESS``"
msgstr ""

#: ../../../Remarks.rst:322
msgid "The hotness of the remark. This record is optional."
msgstr ""

#: ../../../Remarks.rst:325
msgid "Hotness | VBR8 (string table index)"
msgstr ""

#: ../../../Remarks.rst:328
msgid "``RECORD_REMARK_ARG_WITH_DEBUGLOC``"
msgstr ""

#: ../../../Remarks.rst:330 ../../../Remarks.rst:346
msgid "A remark argument with an associated debug location."
msgstr ""

#: ../../../Remarks.rst:333 ../../../Remarks.rst:349
msgid "Key"
msgstr ""

#: ../../../Remarks.rst:335 ../../../Remarks.rst:351
msgid "Value"
msgstr ""

#: ../../../Remarks.rst:344
msgid "``RECORD_REMARK_ARG_WITHOUT_DEBUGLOC``"
