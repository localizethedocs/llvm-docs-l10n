#: ../../../Remarks.rst:313 ../../../Remarks.rst:337
msgid "File"
msgstr ""

#: ../../../Remarks.rst:313 ../../../Remarks.rst:333 ../../../Remarks.rst:335
#: ../../../Remarks.rst:337 ../../../Remarks.rst:349 ../../../Remarks.rst:351
msgid "VBR7 (string table index)"
