#: ../../../AMDGPUUsage.rst:1198
msgid "llvm.amdgcn.log"
msgstr ""

#: ../../../AMDGPUUsage.rst:1198
msgid ""
"Provides direct access to v_log_f32 and v_log_f16 (on targets with half "
"support). Performs log2 function."
msgstr ""

#: ../../../AMDGPUUsage.rst:1201
msgid "llvm.amdgcn.exp2"
msgstr ""

#: ../../../AMDGPUUsage.rst:1201
