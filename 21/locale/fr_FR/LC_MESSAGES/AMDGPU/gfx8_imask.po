msgstr ""

#: ../../../AMDGPU/gfx8_imask.rst:34
msgid ""
"Enable indexing for the specified *operands* and disable it for the rest. "
"*Operand list* is a comma-separated list of values which may include:"
msgstr ""

#: ../../../AMDGPU/gfx8_imask.rst:39
msgid "SRC0 - enable *src0* indexing."
msgstr ""

#: ../../../AMDGPU/gfx8_imask.rst:41
msgid "SRC1 - enable *src1* indexing."
msgstr ""

#: ../../../AMDGPU/gfx8_imask.rst:43
msgid "SRC2 - enable *src2* indexing."
msgstr ""

#: ../../../AMDGPU/gfx8_imask.rst:45
msgid "DST  - enable *dst* indexing."
msgstr ""

#: ../../../AMDGPU/gfx8_imask.rst:47
msgid "Each of these values may be specified only once."
msgstr ""

#: ../../../AMDGPU/gfx8_imask.rst:50
msgid ""
"*Operand list* may be empty; this syntax disables indexing for all operands."
msgstr ""

