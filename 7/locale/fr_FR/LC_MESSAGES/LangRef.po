msgstr ""

#: ../../../LangRef.rst:2604
msgid "``fp128``"
msgstr ""

#: ../../../LangRef.rst:2605
msgid "128-bit floating-point value (112-bit mantissa)"
msgstr ""

#: ../../../LangRef.rst:2607
msgid "``x86_fp80``"
msgstr ""

#: ../../../LangRef.rst:2608
msgid "80-bit floating-point value (X87)"
msgstr ""

#: ../../../LangRef.rst:2610
msgid "``ppc_fp128``"
msgstr ""

#: ../../../LangRef.rst:2611
