msgid "X86:"
msgstr ""

#: ../../../LangRef.rst:3873
msgid "``I``: An immediate integer between 0 and 31."
msgstr ""

#: ../../../LangRef.rst:3874
msgid "``J``: An immediate integer between 0 and 64."
msgstr ""

#: ../../../LangRef.rst:3875
msgid "``K``: An immediate signed 8-bit integer."
msgstr ""

