#: ../../../AMDGPUModifierSyntax.rst:108
msgid "\"1\" - set bit to 1."
msgstr ""

#: ../../../AMDGPUModifierSyntax.rst:110
msgid "\"p\" - preserve bit."
msgstr ""

#: ../../../AMDGPUModifierSyntax.rst:112
msgid "\"i\" - inverse bit."
msgstr ""

